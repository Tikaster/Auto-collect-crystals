local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local collectionRadius = 50
local function isPinkCrystal(object)
    return object.Name == "PinkCrystal"
end
local function moveToCrystal(crystal)
    humanoidRootPart.CFrame = crystal.CFrame
end
while true do
    for _, object in pairs(workspace:GetChildren()) do
        if isPinkCrystal(object) and (object.Position - humanoidRootPart.Position).magnitude <= collectionRadius then
            moveToCrystal(object)
        end
    end
    wait(1)
end
